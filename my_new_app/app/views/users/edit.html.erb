<%= render 'shared/nav' %>

<main>
  <div class="main-div">
    <h1>Edit User</h1>

    <%= form_with(model: @user, local: true) do |form| %>
      <%= form.label :name %>
      <%= form.text_field :name %>

      <%= form.label :email %>
      <%= form.email_field :email %>

      <%= form.submit 'Update User' %>
    <% end %>
  </div>
</main>
